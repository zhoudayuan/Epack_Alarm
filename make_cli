#!/bin/sh

Result_Path=`pwd`/result

Cli_Path=`pwd`/cli

gcc_path=`pwd`/tools/gcc-linaro-arm-linux-gnueabihf-4.9-2014.09_linux/bin

cd $Cli_Path
#Application
$gcc_path/arm-linux-gnueabihf-g++ -O2 -Wall -DLINUX -o cli cli.c -I ../inc -lpthread

if [ -e cli ]
then
	cp cli $Result_Path
	rm cli
fi

